    "@typescript-eslint/parser": "^3.10.1",
    "@types/gl-matrix": "^2.4.5",
    "gl-matrix": "^3.3.0",
    "autoprefixer": "^9.8.6",
    "css-loader": "^3.5.2",
    "eslint": "^7.9.0",
